/* pages/member/resume/resume.wxss */
.clear {
  clear:both;
}
.text1 .left {
  float: left;
}
.text1 .right {
  float: right;
}
.content1 {
  padding: 10px;
  border-bottom: 10px solid #f1f1f1;
  height: 178rpx;
  box-sizing: border-box;
}
.content1-bottom {
  height: 100%;
}
.content1-bottom .img {
  float: right;
  vertical-align:middle;
  display: inline-block;
  border-radius: 50%;
  overflow: hidden;
}
.content1-bottom .img image {
  border-radius: 50%;
}
.content1-bottom .text6{
  float: right;
  line-height: 118rpx;
  margin-left: 30rpx;
}
.content1-bottom .img image {
  width: 110rpx;
  height: 110rpx;
}
.content1-bottom .info {
  float: left;
  vertical-align:middle;
  margin-left: 5px;
  height: 100%;
}
.content1-bottom .info .info-top {
  display: inline-block;
  height: 100%;
}
.content1-bottom .info .info-top .text1 {
  display: inline-block;
  font-weight: bold;
  vertical-align:middle;
  height: 100%;
  line-height: 118rpx;
  color: #707070;
  font-size: 32rpx;
}
.content1-bottom .info .info-bottom {
  font-size: 12px;
}

.content2 {
  padding: 10px;
  border-bottom: 10px solid #f1f1f1;
}
.content2 .content2-name {
  font-weight: bold;
  margin: 10px 0 30px 0;
}
.content2 .text1 {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  border-bottom: 2rpx solid #e6e6e6;
}
.content2 .text1 input {
  text-align:right;
  /* background-color: pink; */
  margin-top: 25rpx;
}
.content2 .text1 radio {
  margin-left: 15rpx;
}