/* pages/member/resume/resume.wxss */
.clear {
  clear:both
}
.content1 {
  padding: 10px;
  border-bottom: 10px solid #f1f1f1;
}
.content1-top {
  padding: 15px 0;
}
.content1-top .text1 {
  float: left;
  font-weight: bold;
}
.content1-top .text2 {
  float: right;
  color: #7e7e7e;
  font-size: 14px;
}
.content1-bottom .img {
  display: inline-block;
  vertical-align:middle;

}
.content1-bottom .img image {
  width: 55px;
  height: 55px;
}
.content1-bottom .info {
  display: inline-block;
  vertical-align:middle;
  margin-left: 5px;
}
.content1-bottom .info .info-top {
  display: inline-block;
}
.content1-bottom .info .info-top .text1 {
  display: inline-block;
  font-weight: bold;
}
.content1-bottom .info .info-top .text2 {
  display: inline-block;
  color: #7e7e7e;
  background-color: #f1f1f1;
  border-radius: 10px;
  margin-left: 3px;
  padding: 3px 6px;
  font-size: 12px;
  margin-left: 5px;
}
.content1-bottom .info .info-bottom {
  font-size: 12px;
}

.content2 {
  padding: 10px;
  border-bottom: 10px solid #f1f1f1;
}
.content2 .content2-name {
  font-weight: bold;
  margin: 10px 0 30px 0;
}
.content2 .content2-item {
  width: 100%;
  background-color: #f5f5f5;
  padding: 30px 0;
}
.content2 .content2-item .text1 {
  color: #fba908;
  text-align: center;
  font-size: 14px;
}
.content2 .content2-item .text1 text {
  font-size: 18px;
  display: inline-block;
}
.content2 .content2-item .text2 {
  color: #b1b1b1;
  font-size: 14px;
  text-align: center;
}